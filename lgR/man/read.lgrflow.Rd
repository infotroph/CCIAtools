## The function is currently defined as
function (file) 
{
    if (any(grep("\\.txt\\.zip$", file))) {
        method = "unzip -p"
    }
    else {
        method = "cat"
    }
    command = paste(method, file, "| sed -e '/-----BEGIN PGP MESSAGE-----/{x;q;}'", 
        "-e '/#=-=-=-=-=-=-=-=-=Start/{x;q;}'")
    read.csv(pipe(command), skip = 1)
  }
}
